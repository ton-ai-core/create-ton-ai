{
    "name": "{{name}}",
    "version": "0.0.1",
    "scripts": {
        "bp": "blueprint",
        "prebuild": "npx -y @ton-ai-core/blueprint-linter",
        "prebuild\\w+": "npx -y @ton-ai-core/blueprint-linter",
        "build": "blueprint build",
        "postbuild:\\w+": "node insert-c8-ignore.js && blueprint misti $BLUEPRINT_SCRIPT_NAME && node insert-c8-ignore.js",
        "postbuild": "node insert-c8-ignore.js && blueprint misti --all && node insert-c8-ignore.js",
        "prerun:deploy\\w+": "blueprint generate-specs && blueprint test",
        "prerun:\\w+": "blueprint generate-specs && blueprint test ./tests/$BLUEPRINT_SCRIPT_NAME.spec.ts",
        "start": "blueprint run",
        "pretest": "npx rimraf ./build && npx blueprint build --all && eslint . --ext .ts",
        "test": "jest --verbose",
        "lint": "eslint . --ext .ts",
        "misti": "blueprint misti",
        "coverage": "jest --coverage"
    },
    "devDependencies": {
        "@ton-ai-core/blueprint-misti": "^0.8.9",
        "@ton-ai-core/blueprint-sandbox": "^0.1.8",
        "@ton-ai-core/blueprint": "^0.34.5",
        "@ton-ai-core/eslint-plugin-suggest-members": "^1.5.4",
        "@typescript-eslint/eslint-plugin": "^8.30.1",
        "@typescript-eslint/parser": "^8.30.1",
        "eslint": "^8.57.0",
        "@ton/sandbox": "^0.31.0",
        "@ton/test-utils": "^0.6.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.14.0",
        "jest": "^29.7.0",
        "prettier": "^3.5.3",
        "@ton/ton": "^15.2.1",
        "@ton/core": "~0",
        "@ton/crypto": "^3.3.0",
        "ts-jest": "^29.3.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "@ton/tolk-js": "^0.12.0",
        "@tact-lang/compiler": "^1.6.11",
        "@ton-community/func-js": "^0.9.1"
    }
}
